
# Python3 code here creating class
class goods:
    def __init__(self, name, roll):
        self.name = name
        self.roll = roll
 
# creating list
list = []
 
# appending instances to list
list.append(goods('Akash', 2))
list.append(goods('Deependra', 40))
list.append(goods('Reaper', 44))
list.append(goods('veer', 67))
 
# Accessing object value using a for loop
for obj in list:
    print(obj.name, obj.roll, sep=' ')
 
print("")
# Accessing individual elements
print(list[0].name)
print(list[1].name)
print(list[2].name)
print(list[3].name)
